<%= link_to "View all books", "/books" %> 
<h1>Libraries</h1>
<% @libraries.order_by_recent.each_with_index do |library, index| %>
    <div id="library-<%= index %>">
        <p>
            <%= library.name %> created: <%= library.created_at %> 
            <%= link_to "Update #{library.name} Library", "/libraries/#{library.id}/edit"%>
            <br>
            <%= link_to "Delete #{library.name} Library", "/libraries/#{library.id}", method: :delete %>        
        </p>
    </div>
<% end %>
<%= link_to "New Library", "/libraries/new"%>
