<% @books.each do |book| %>
    <h3><%=book.title%></h3>
    <p>ID: <%=book.id%></p>
    <p>Author: <%=book.author%></p>
    <p>Copyright: <%=book.copyright%></p>
    <p>Available: <%=book.available ? "Yes" : "No" %></p>
    <p>Created: <%=book.created_at %></p>
    <p>Updated: <%=book.updated_at %></p>
<% end %>
